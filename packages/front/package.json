{
  "name": "@ac-extreme-mercenaries/front",
  "version": "1.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "VITE_GIT_HASH=$(git rev-parse --short HEAD) react-router build",
    "build:test": "react-router build --mode test",
    "dev": "react-router dev",
    "dev:test": "react-router dev --mode test",
    "start": "wrangler pages dev ./build/client",
    "preview": "pnpm run build && wrangler pages dev",
    "deploy": "pnpm run build && wrangler pages deploy",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --coverage --run",
    "typecheck": "react-router typegen && tsc",
    "lint": "run-s run-eslint run-prettier",
    "run-eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "run-prettier": "prettier --write .",
    "typegen": "wrangler types && react-router typegen",
    "cf-typegen": "wrangler types",
    "migration": "wrangler d1 migrations apply ac-extreme-mercenaries-db --local",
    "migration:prod": "wrangler d1 migrations apply ac-extreme-mercenaries-db --remote",
    "migration:gen": "drizzle-kit generate",
    "migration:test": "wrangler d1 migrations apply ac-extreme-mercenaries-db-test --local --env test --config wrangler.test.toml",
    "export:prod": "wrangler d1 export ac-extreme-mercenaries-db --remote --no-schema",
    "seed:local": "tsx app/db/seeds/local/runner.ts",
    "sql": "wrangler d1 execute ac-extreme-mercenaries-db --local",
    "sql:prod": "wrangler d1 execute ac-extreme-mercenaries-db --remote",
    "sql:test": "wrangler d1 execute ac-extreme-mercenaries-db-test --local --env test --config wrangler.test.toml",
    "clear:local": "wrangler d1 execute ac-extreme-mercenaries-db --local --file ./app/db/queries/clear-records.sql",
    "clear:prod": "wrangler d1 execute ac-extreme-mercenaries-db --remote --file ./app/db/queries/clear-records.sql",
    "secrets:gen": "openssl rand -base64 32"
  },
  "dependencies": {
    "@date-fns/tz": "1.4.1",
    "@heroicons/react": "2.2.0",
    "@react-router/cloudflare": "7.9.2",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.44.5",
    "highlight.js": "11.11.1",
    "html-react-parser": "5.2.3",
    "isbot": "^5.0.0",
    "jsdom": "27.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.63.0",
    "react-router": "7.9.2",
    "sanitize-html": "2.17.0",
    "uuid": "13.0.0",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250922.0",
    "@fast-check/vitest": "0.2.2",
    "@react-router/dev": "7.9.2",
    "@react-router/node": "7.9.2",
    "@tailwindcss/postcss": "4.1.13",
    "@types/jsdom": "21.1.7",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@types/sanitize-html": "2.16.0",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.21",
    "drizzle-kit": "0.31.4",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "fast-check": "4.3.0",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.13",
    "wrangler": "4.40.1"
  },
  "optionalDependencies": {
    "@cloudflare/workerd-darwin-64": "1.20251004.0",
    "@cloudflare/workerd-darwin-arm64": "1.20250924.0",
    "@cloudflare/workerd-linux-64": "1.20250924.0",
    "@rollup/rollup-darwin-arm64": "4.52.2",
    "@rollup/rollup-darwin-x64": "4.52.2",
    "@rollup/rollup-linux-arm64-gnu": "4.52.2",
    "@rollup/rollup-linux-x64-gnu": "4.52.2",
    "lightningcss-darwin-arm64": "1.30.1",
    "lightningcss-darwin-x64": "1.30.1",
    "lightningcss-linux-x64-gnu": "1.30.1"
  },
  "overrides": {
    "workerd": {
      "optionalDependencies": {
        "@cloudflare/workerd-linux-64": "1.20250903.0",
        "@cloudflare/workerd-darwin-64": "1.20250919.0",
        "@cloudflare/workerd-darwin-arm64": "1.20250922.0"
      }
    }
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
