{
  "name": "@ac-extreme-mercenaries/front",
  "version": "1.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "wrangler pages dev ./build/client",
    "preview": "npm run build && wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --coverage --run",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --fix --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typegen": "wrangler types && react-router typegen",
    "migration:gen": "drizzle-kit generate",
    "migration": "wrangler d1 migrations apply ac-extreme-mercenaries-db --local",
    "migration:prod": "wrangler d1 migrations apply ac-extreme-mercenaries-db --remote",
    "build:test": "react-router build --mode test",
    "migration:test": "wrangler d1 migrations apply ac-extreme-mercenaries-db-test --local --env test --config wrangler.test.toml",
    "dev:test": "react-router dev --mode test",
    "db-execute:test": "wrangler d1 execute ac-extreme-mercenaries-db-test --local --env test --config wrangler.test.toml",
    "cf-typegen": "wrangler types",
    "secrets:gen": "openssl rand -base64 32"
  },
  "dependencies": {
    "@date-fns/tz": "1.2.0",
    "@googleapis/youtube": "20.0.0",
    "@react-router/cloudflare": "^7.0.0",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.39.1",
    "highlight.js": "11.11.1",
    "html-react-parser": "5.2.2",
    "isbot": "^5.0.0",
    "jsdom": "26.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.54.2",
    "react-router": "^7.0.0",
    "remix-sitemap": "^3.2.0",
    "sanitize-html": "2.14.0",
    "uuid": "11.0.5",
    "vite-env-only": "3.0.3",
    "zod": "3.24.1",
    "zodix": "0.4.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250129.0",
    "@fast-check/vitest": "0.1.5",
    "@react-router/dev": "7.1.3",
    "@types/jsdom": "21.1.7",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/sanitize-html": "2.13.0",
    "@typescript-eslint/eslint-plugin": "8.22.0",
    "@typescript-eslint/parser": "8.22.0",
    "@vitest/coverage-v8": "3.0.4",
    "autoprefixer": "10.4.20",
    "drizzle-kit": "0.30.4",
    "eslint": "8.57.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "fast-check": "3.23.2",
    "npm-run-all2": "7.0.2",
    "postcss": "8.5.1",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "vite": "5.4.14",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.4",
    "wrangler": "3.106.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
