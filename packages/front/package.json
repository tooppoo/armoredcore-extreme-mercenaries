{
  "name": "@ac-extreme-mercenaries/front",
  "version": "1.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --coverage --run",
    "deploy": "npm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --fix --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "start:dev": "wrangler pages dev --local",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "migration:gen": "drizzle-kit generate",
    "migration": "wrangler d1 migrations apply ac-extreme-mercenaries-db --local",
    "migration:prod": "wrangler d1 migrations apply ac-extreme-mercenaries-db --remote",
    "cf-typegen": "wrangler types",
    "secrets:gen": "openssl rand -base64 32"
  },
  "dependencies": {
    "@date-fns/tz": "1.2.0",
    "@googleapis/youtube": "20.0.0",
    "@remix-run/cloudflare": "^2.15.2",
    "@remix-run/cloudflare-pages": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.38.3",
    "highlight.js": "11.11.1",
    "html-react-parser": "5.2.2",
    "isbot": "^5.0.0",
    "jsdom": "26.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.54.2",
    "remix-sitemap": "^3.2.0",
    "sanitize-html": "2.14.0",
    "uuid": "11.0.5",
    "vite-env-only": "3.0.3",
    "zod": "3.24.1",
    "zodix": "0.4.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250109.0",
    "@fast-check/vitest": "0.1.4",
    "@remix-run/dev": "2.15.2",
    "@types/jsdom": "21.1.7",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/sanitize-html": "2.13.0",
    "@typescript-eslint/eslint-plugin": "8.20.0",
    "@typescript-eslint/parser": "8.20.0",
    "@vitest/coverage-v8": "2.1.8",
    "autoprefixer": "10.4.20",
    "drizzle-kit": "^0.30.1",
    "eslint": "8.57.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.3",
    "eslint-plugin-react-hooks": "4.6.2",
    "fast-check": "3.23.2",
    "postcss": "8.4.49",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "vite": "5.4.11",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.1.8",
    "wrangler": "3.101.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
