{
  "name": "@ac-extreme-mercenaries/front",
  "version": "1.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:test": "react-router build --mode test",
    "dev": "react-router dev",
    "dev:test": "react-router dev --mode test",
    "start": "wrangler pages dev ./build/client",
    "preview": "npm run build && wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --coverage --run",
    "typecheck": "react-router typegen && tsc",
    "lint": "run-s run-eslint run-prettier",
    "run-eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "run-prettier": "prettier --write .",
    "typegen": "wrangler types && react-router typegen",
    "cf-typegen": "wrangler types",
    "migration": "wrangler d1 migrations apply ac-extreme-mercenaries-db --local",
    "migration:prod": "wrangler d1 migrations apply ac-extreme-mercenaries-db --remote",
    "migration:gen": "drizzle-kit generate",
    "migration:test": "wrangler d1 migrations apply ac-extreme-mercenaries-db-test --local --env test --config wrangler.test.toml",
    "export:prod": "wrangler d1 export ac-extreme-mercenaries-db --remote --no-schema",
    "seed:local": "tsx app/db/seeds/local/runner.ts",
    "sql": "wrangler d1 execute ac-extreme-mercenaries-db --local",
    "sql:prod": "wrangler d1 execute ac-extreme-mercenaries-db --remote",
    "sql:test": "wrangler d1 execute ac-extreme-mercenaries-db-test --local --env test --config wrangler.test.toml",
    "clear:local": "wrangler d1 execute ac-extreme-mercenaries-db --local --file ./app/db/queries/clear-records.sql",
    "clear:prod": "wrangler d1 execute ac-extreme-mercenaries-db --remote --file ./app/db/queries/clear-records.sql",
    "secrets:gen": "openssl rand -base64 32"
  },
  "dependencies": {
    "@date-fns/tz": "1.2.0",
    "@googleapis/youtube": "25.0.0",
    "@heroicons/react": "2.2.0",
    "@react-router/cloudflare": "^7.0.0",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.43.1",
    "highlight.js": "11.11.1",
    "html-react-parser": "5.2.3",
    "isbot": "^5.0.0",
    "jsdom": "26.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.56.4",
    "react-router": "^7.0.0",
    "remix-sitemap": "^3.2.0",
    "sanitize-html": "2.17.0",
    "uuid": "11.1.0",
    "zod": "3.25.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250601.0",
    "@eslint/compat": "1.2.9",
    "@fast-check/vitest": "0.2.1",
    "@react-router/dev": "7.5.2",
    "@tailwindcss/postcss": "4.1.8",
    "@types/jsdom": "21.1.7",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "@types/sanitize-html": "2.16.0",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "@vitest/coverage-v8": "3.1.4",
    "autoprefixer": "10.4.21",
    "drizzle-kit": "0.31.1",
    "eslint": "9.27.0",
    "eslint-import-resolver-typescript": "4.3.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "fast-check": "4.1.1",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.4",
    "prettier": "3.5.3",
    "tailwindcss": "4.1.8",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.4",
    "wrangler": "4.16.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
