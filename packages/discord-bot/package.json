{
  "name": "@ac-extreme-mercenaries/discord-bot",
  "version": "1.0.0",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "start": "tsx src/app.ts",
    "dev": "dotenvx run -f .env.local -f .env -- tsx watch src/app.ts",
    "deploy:commands": "tsx scripts/deploy-commands.ts",
    "build": "tsc",
    "lint": "run-s run-eslint run-prettier",
    "run-eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "run-prettier": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.32.1",
    "@hono/node-server": "^1.13.7",
    "discord.js": "14.19.3",
    "dotenv": "16.5.0",
    "hono": "^4.6.16"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.0",
    "@types/node": "22.15.31",
    "tsx": "4.20.2",
    "eslint": "9.28.0",
    "eslint-import-resolver-typescript": "4.4.3",
    "eslint-plugin-import": "2.31.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.5.3"
  }
}
