{
  "name": "@ac-extreme-mercenaries/discord-bot",
  "version": "1.0.0",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "start": "node build/index.js",
    "dev": "dotenvx run -f .env.local -f .env -- tsx watch src/app.ts",
    "deploy:commands": "tsx scripts/deploy-commands.ts",
    "build": "tsc",
    "lint": "run-s run-eslint run-prettier",
    "run-eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "run-prettier": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "discord.js": "14.22.1",
    "hono": "^4.9.7"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.32.1",
    "@types/node": "22.18.3",
    "dotenv": "17.2.2",
    "tsx": "4.20.5",
    "prettier": "3.6.2"
  }
}